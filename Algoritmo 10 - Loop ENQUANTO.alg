algoritmo "Algoritmo 10 - Loop Enquanto"
var
  palavra: caractere
  contador: inteiro
inicio
  palavra <- "Senac - Algoritmo"
  contador <- 1
  
  enquanto (contador <= 10) faca
    escreval(palavra)
    
    // Atualização da variável contadora
    contador <- contador + 1
  fimenquanto

fimalgoritmo
