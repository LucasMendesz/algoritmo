algoritmo "Operador NÃO (NEGAÇÃO)"
// Função : INVERTE A LÓGICA
var
  blackFriday: logico  // VERDADEIRO OU FALSO
inicio

  blackFriday <- falso
  
  // SE BLACKFRIDAY NÃO É VERDADEIRA
  se nao (blackFriday) entao
     escreva("Não estamos na black friday... :(")
  senao
     escreva("Estamos na black friday... :)")
  fimse

fimalgoritmo
